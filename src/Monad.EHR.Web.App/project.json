{
    "webroot": "wwwroot",
    "version": "1.0.0-*",
    "dependencies": {
        "Microsoft.AspNet.IISPlatformHandler": "1.0.0-beta8",
        "Microsoft.AspNet.Mvc": "6.0.0-beta8",
        "Microsoft.AspNet.Server.Kestrel": "1.0.0-beta8",
        "Microsoft.AspNet.Server.WebListener": "1.0.0-beta8",
        "Microsoft.AspNet.StaticFiles": "1.0.0-beta8",
        "EntityFramework.SqlServer": "7.0.0-beta8",
        "EntityFramework.Commands": "7.0.0-beta8",
        "Microsoft.Framework.Configuration": "1.0.0-beta8",
        "Microsoft.Framework.Configuration.Abstractions": "1.0.0-beta8",
        "Microsoft.Framework.Configuration.Json": "1.0.0-beta8",
        "Microsoft.AspNet.Session": "1.0.0-beta8",
        "Microsoft.Framework.Caching.Memory": "1.0.0-beta8",
        "Microsoft.AspNet.Routing": "1.0.0-beta8",
        "Monad.EHR.Domain.Entities": "1.0.0-*",
        "Monad.EHR.Domain.Interfaces": "1.0.0-*",
        "Monad.EHR.Services.Interface": "1.0.0-*",
        "Monad.EHR.Infrastructure.DependencyResolver": "1.0.0-*",
        "Microsoft.AspNet.Mvc.WebApiCompatShim": "6.0.0-beta8",
        "Microsoft.AspNet.Diagnostics": "1.0.0-beta8"
    },
    "commands": {
      "web":  "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --server.urls http://localhost:9999",
        "kestrel": "Microsoft.AspNet.Hosting --server Kestrel --config hosting.ini"
    },
    "frameworks": {
        "dnx451": {}
    },
    "exclude": ["wwwroot", "node_modules", "bower_components"],
    "publishExclude": ["node_modules", "bower_components", "**.xproj", "**.user", "**.vspscc"],
    "scripts": {
        "prepublish": ["npm install", "bower install"]
    }
}
