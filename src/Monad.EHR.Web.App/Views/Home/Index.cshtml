<!DOCTYPE html>
<html ng-app="mainModule" ng-controller="defaultController">
<head>

    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>{{title}}</title>

    <!-- Bootstrap core CSS -->
    <link href="css/bootstrap.min.css" rel="stylesheet">
    <link href="fonts/css/font-awesome.min.css" rel="stylesheet">
    <link href="css/animate.min.css" rel="stylesheet">
    <!-- Custom styling plus plugins -->
    <link href="css/custom.css" rel="stylesheet">
    <link href="css/icheck/flat/green.css" rel="stylesheet">
    <script src="js/jquery.min.js"></script>
    <link href="lib/angular-loading-bar/build/loading-bar.min.css" rel="stylesheet">
</head>

<body class="nav-md" ng-init="initializeController()">
    <div class="container body">

        <div class="main_container">
            <div class="col-md-3 left_col">
                <div class="left_col scroll-view">
                    <div class="navbar nav_title" style="border: 0;">
                        <a href="#/Home" class="site_title"><span>EHR</span></a>
                    </div>
                    <div class="clearfix"></div>
                    <!-- menu prile quick info -->
                    <div class="profile">
                        <div class="profile_pic">
                            <img ng-src="data:image/jpeg;base64,{{User.imageData}}" class="img-circle profile_img" />
                            <!--<img src="images/img.jpg" alt="..." class="img-circle profile_img">-->
                        </div>
                        <div class="profile_info">
                            <span>Welcome,</span>
                            <h2>{{User.FirstName}} {{User.LastName}}</h2>
                        </div>
                    </div>
                    <!-- /menu prile quick info -->
                    <br />
                    <!-- sidebar menu -->
                    <div class="clearfix"></div>
                    <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
                        <div class="menu_section">
                            <ul class="nav side-menu">
                                <li>
                                    <a href="#/Home"><i class="fa fa-home"></i> Home <span class="fa fa-chevron-down"></span></a>

                                </li>
                                <li>
                                    <a><i class="fa fa-edit"></i> Forms <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li><a href = "/#/viewPatient">Patient</a></li>

                                    </ul>
                                </li>

                            </ul>
                        </div>

                    </div>
                    <!-- /sidebar menu -->

                </div>
            </div>
            <!-- top navigation -->
            <div class="top_nav">
                <div class="nav_menu">
                    <nav class="" role="navigation">
                        <div class="nav toggle">
                            <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                        </div>
                        <ul class="nav navbar-nav navbar-right">
                            <li class="">
                                <a href="javascript:;" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                    <img ng-src="data:image/jpeg;base64,{{User.imageData}}" />
                                    <!--<img src="images/img.jpg" alt="">-->{{User.FirstName}} {{User.LastName}}
                                    <span class=" fa fa-angle-down"></span>
                                </a>
                                <ul class="dropdown-menu dropdown-usermenu animated fadeInDown pull-right">
                                    <li>
                                        <a ng-click="showUserProfile()"> <i class="fa fa-sign-out pull-right"></i> Profile</a>
                                    </li>
                                    <li>
                                        <a ng-click="logout()"> <i class="fa fa-sign-out pull-right"></i> Log Out</a>
                                    </li>
                                </ul>
                            </li>

                        </ul>
                    </nav>
                </div>
            </div>
            <!-- /top navigation -->
            <!-- page content -->
            <div class="right_col" role="main">

                <div class="row" ng-view>
                    <div class="clearfix"></div>
                </div>
                <!-- footer content -->
                <footer>
                    <div class="">
                        <p class="pull-right">
                            Dummy Inc

                        </p>
                    </div>
                    <div class="clearfix"></div>
                </footer>
                <!-- /footer content -->
            </div>
            <!-- /page content -->
        </div>
    </div>

    <script src="js/bootstrap.min.js"></script>
    <!-- bootstrap progress js -->
    <script src="js/progressbar/bootstrap-progressbar.min.js"></script>
    <script src="js/nicescroll/jquery.nicescroll.min.js"></script>
    <!-- icheck -->
    <script src="js/icheck/icheck.min.js"></script>
    <script src="js/custom.js"></script>
    <!-- moris js -->
    <script src="lib/angular/angular.js"></script>
    <script src="lib/angular-route/angular-route.js"></script>
    <script src="lib/angular-cookies/angular-cookies.min.js"></script>
    <script src="lib/angular-block-ui/dist/angular-block-ui.min.js"></script>
    <script src="lib/angular-resource/angular-resource.min.js"></script>
    <script src="lib/angular-sanitize/angular-sanitize.js"></script>
    <script src="lib/angular-ui/build/angular-ui.min.js"></script>
    <script src="lib/angular-confirm-modal/angular-confirm.js"></script>
    <script src="lib/angular-loading-bar/build/loading-bar.js"></script>


    <script src="app/app.js"></script>
    <script src="app/Services/applicationService.js"></script>
    <script src="app/Services/accountsService.js"></script>
    <script src="app/Services/interceptorService.js"></script>
    <script src="app/Services/tokenHandlerService.js"></script>
    <script src="app/Services/userService.js"></script>
    <script src="lib/angular-validation/dist/angular-validation.min.js"></script>
    <script src="lib/angular-validation/dist/angular-validation-rule.min.js"></script>
    <script src="lib/angular-base64-upload/src/angular-base64-upload.js"></script>
    <script src="lib/angular-smart-table/dist/smart-table.min.js"></script>

    <script src="app/Services/alertsService.js"></script>
    <script src="app/Services/patientService.js"></script>
<script src="app/Services/addressService.js"></script>
<script src="app/Services/medicationsService.js"></script>
<script src="app/Services/problemsService.js"></script>
<script src="app/Services/bPService.js"></script>
<script src="app/Services/patientHeightService.js"></script>
<script src="app/Services/weightService.js"></script>

    <script src="app/home/controllers/deletepopupdirective.js"></script>
    <script src="app/Accounts/Controllers/userController.js"></script>
    <script src="app/Home/Controllers/homeController.js"></script>
    <script src="app/Patient/Controllers/patientController.js"></script>
<script src="app/Address/Controllers/addressController.js"></script>
<script src="app/Medications/Controllers/medicationsController.js"></script>
<script src="app/Problems/Controllers/problemsController.js"></script>
<script src="app/BP/Controllers/bPController.js"></script>
<script src="app/PatientHeight/Controllers/patientHeightController.js"></script>
<script src="app/Weight/Controllers/weightController.js"></script>

    <script language="JavaScript">

        function init() {
        }
    </script>
</body>

</html>


